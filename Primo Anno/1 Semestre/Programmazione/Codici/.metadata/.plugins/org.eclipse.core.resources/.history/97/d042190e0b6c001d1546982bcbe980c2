#include <stdio.h>
#include <stdlib.h>
void StampaValori(int vettore[],int n)
{
	int pos=0;
	while(pos<n)
	{
		printf(" %d\n",vettore[pos]);
		pos=pos+1;
	}
	return;
}
int InputN(int n)
{
	printf("Indicami il valore di N: ");fflush(stdout);
    scanf(" %d",&n);
 	return n;
}

int main(void) {
	int N=1;
	int K=1;
	int Pos=0;
	int Val=1;
	N=InputN(N);
	int *V=calloc(N,sizeof(int));
	while(Pos<N)
	{
		V[Pos]=Val*K;
		Val=Val+1;
		Pos=Pos+1;
	}
	StampaValori(V,N);
	system("pause");
	return EXIT_SUCCESS;
}

